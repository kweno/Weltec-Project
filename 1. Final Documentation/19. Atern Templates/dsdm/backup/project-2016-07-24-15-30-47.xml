  <cats>
    <cat name="dsdm" class="project DSDMAtern">
      <cat name="Lifecycle" class="process">
        <cat name="Pre-Project" class="subprocess">
          <doc id="17" name="Terms of Reference">
            <extendedproperties tip="Produced as a high level definition of the business driver and objectives of the project.&#xD;&#xA;May be as simple as an email or the note of a verbal agreement.&#xD;&#xA;Should be APPROVED by the project sponsor." />
          </doc>
          <doc id="12" name="Project Roles Summary">
            <extendedproperties tip="Complete with details of who is or are expected to fulfil which rolls on the project.&#xD;&#xA;At this stage may only be the Project Sponsor." />
          </doc>
        </cat>
        <cat name="Feasibility" class="subprocess">
          <doc id="17" name="Terms of Reference">
            <extendedproperties tip="Updated and used as an input into Feasibility Assessment and Outline Plan." />
          </doc>
          <doc id="9" name="Outline Plan">
            <extendedproperties tip="Outline of governance arrangements and organisation structure, key deliverables&#xD;&#xA;provisional estimates and schedule for the project.  To be fleshed out in the&#xD;&#xA;Foundation stage.&#xD;&#xA;PRODUCED by the Project Manager&#xD;&#xA;APPROVED by the Project Sponsor" />
          </doc>
          <doc id="5" name="Feasibility Assessment">
            <extendedproperties tip="To provide a high level overview of the project and its business and technical feasibility.&#xD;&#xA;Usually PRODUCED by the Business Analyst with inputs from others as required.&#xD;&#xA;APPROVED by the Project Sponsor" />
          </doc>
          <doc id="12" name="Project Roles Summary">
            <extendedproperties tip="Complete with details of who is or are expected to fulfil which rolls on the project.&#xD;&#xA;At this stage may include the Project Manager, Business Analyst and other key rolls." />
          </doc>
        </cat>
        <cat name="Foundations" class="subprocess">
          <doc id="5" name="Feasibility Assessment">
            <extendedproperties tip="Updated and used as an input to the Business Foundations then archived." />
          </doc>
          <doc id="2" name="Business Foundations">
            <extendedproperties tip="Provides information for and/or about the Business which is fundamental to the&#xD;&#xA;success of the project and which must be understood by all involved.&#xD;&#xA;Comprising the Business Vision and Business Case.&#xD;&#xA;PRODUCED by the Business Analyst&#xD;&#xA;APPROVED by the Business Sponsor" />
          </doc>
          <doc id="16" name="Solution Foundations">
            <extendedproperties tip="Provides information for and/or about the Solution which is fundamental to the&#xD;&#xA;success of the project and which must be understood by all involved.&#xD;&#xA;Composition depending on project type and scale may include BAD, SAD,DAD&#xD;&#xA;and Solution Prototype, with associated PRODUCED and APPROVED roles." />
          </doc>
          <doc id="7" name="Management Foundations">
            <extendedproperties tip="Describes how the project will be managed and how Atern will be applied to&#xD;&#xA;ensure success.&#xD;&#xA;Note: essential to review Project Approach Questionnaires (found in Outline plan).&#xD;&#xA;PRODUCED by Project Manager&#xD;&#xA;APPROVED  by Business Sponsor" />
          </doc>
          <doc id="9" name="Outline Plan">
            <extendedproperties tip="Updated and used as an input to the Delivery Plan then archived.&#xD;&#xA;Consider copying files from Outline plan to here and fleshing out&#xD;&#xA;rather than recreating." />
          </doc>
          <doc id="4" name="Delivery Plan">
            <extendedproperties tip="Refines and elaborates on the schedule and resources provided in the Outline Plan.&#xD;&#xA;Consider copying files from the outline plan and updating.&#xD;&#xA;PRODUCED by the Project Manager.&#xD;&#xA;APPROVED by the Business Visionary." />
          </doc>
          <doc id="10" name="Prioritised Requirements List">
            <extendedproperties tip="High level description of the requirements the project is to satisfy, indicating their&#xD;&#xA;relative priority.&#xD;&#xA;PRODUCED by the Business Analyst.&#xD;&#xA;APPROVED by the Business Visionary." />
          </doc>
          <doc id="12" name="Project Roles Summary">
            <extendedproperties tip="Complete with details of who is or are expected to fulfil which rolls on the project." />
          </doc>
          <doc id="3" name="Delivery Control Pack">
            <extendedproperties tip="Contains (along with Reports Document below) key logs and other materials related&#xD;&#xA;to the ongoing status of the project.  The team may add other content as they find&#xD;&#xA;helpful including a burn down chart or project dashboard.  PRODUCED by and&#xD;&#xA;APPROVED by varies for each included file." />
          </doc>
          <doc id="13" name="Reports">
            <extendedproperties tip="Contains (along with the Delivery Control Pack Document above) materials related&#xD;&#xA;to the ongoing status of the project.  Status and assurance reports and dashboards&#xD;&#xA;produced by PROJECT in a box can be auto published to here.&#xD;&#xA;PRODUCED by and APPROVED by varies for each included file." />
          </doc>
        </cat>
        <cat name="Exploration and Engineering" class="subprocess">
          <doc id="4" name="Delivery Plan">
            <extendedproperties tip="Used and updated as required." />
          </doc>
          <doc id="10" name="Prioritised Requirements List">
            <extendedproperties tip="Used and updated as required." />
          </doc>
          <doc id="3" name="Delivery Control Pack">
            <extendedproperties tip="Used and updated as required." />
          </doc>
          <doc id="15" name="Solution Assurance">
            <extendedproperties tip="A collection of documents relating to the completeness and fitness of purpose of&#xD;&#xA;the parts of the evolving solution.  May include Solution Review Record, Business&#xD;&#xA;Testing Pack and Technical Testing Pack.&#xD;&#xA;PRODUCED by and APPROVED by roles differ for each document in the pack." />
          </doc>
          <doc id="12" name="Project Roles Summary">
            <extendedproperties tip="Complete with details of who is or are expected to fulfil which rolls on the project." />
          </doc>
        </cat>
        <cat name="Deployment" class="subprocess">
          <doc id="15" name="Solution Assurance">
            <extendedproperties tip="Used and updated as required." />
          </doc>
          <doc id="4" name="Delivery Plan">
            <extendedproperties tip="Used and updated as required." />
          </doc>
          <doc id="3" name="Delivery Control Pack">
            <extendedproperties tip="Used and updated as required." />
          </doc>
          <doc id="11" name="Project Review Report">
            <extendedproperties tip="A live document which accumulates the following: each Increment Review Record&#xD;&#xA;each Benefits Enablement Summary and the End of Project Assessment.&#xD;&#xA;PRODUCED by and APPROVED by roles vary with each document." />
          </doc>
        </cat>
        <cat name="Post-Project" class="subprocess">
          <doc id="1" name="Benefits Assessment">
            <extendedproperties tip="Describes how the benefits have been accrued as the Deployed Solution has been&#xD;&#xA;used.  For projects with longer term benefits this may be produced on an agreed&#xD;&#xA;periodic basis.&#xD;&#xA;PRODUCED by the Buisness Visonary.&#xD;&#xA;ACCEPTED by the Business Sponsor." />
          </doc>
          <doc id="11" name="Project Review Report" />
        </cat>
      </cat>
